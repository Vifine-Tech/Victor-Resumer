{
  "name": "victorkipkoech/portfolio-module",
  "description": "A modern Laravel portfolio module for DevOps professionals",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Victor Kipkoech",
      "email": "victor@victor.co.ke"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0",
    "intervention/image": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "VictorKipkoech\\Portfolio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VictorKipkoech\\Portfolio\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VictorKipkoech\\Portfolio\\PortfolioServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}